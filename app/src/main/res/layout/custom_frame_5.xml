<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/ivframebg"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/frame_05"
        android:adjustViewBounds="true"
        android:orientation="vertical" />

    <ImageView
        android:id="@+id/ivframelogo"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_margin="8dp"
        android:layout_gravity="left" />



    <LinearLayout
        android:id="@+id/ll_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginBottom="@dimen/_30sdp"
        android:layout_gravity="bottom"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/ivEmail"
            android:layout_width="8dp"
            android:layout_height="@dimen/_6sdp"
            android:src="@drawable/s2"
            android:layout_gravity="center"
            android:visibility="gone"
            app:tint="@color/white" />

        <TextView
            android:id="@+id/tvframeemail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_4sdp"
            android:ellipsize="end"
            android:gravity="center"
            android:maxLines="1"
            android:textColor="@color/ColorBlack"
            android:textSize="@dimen/_6sdp"
            android:visibility="gone" />

    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_web"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginStart="@dimen/_20sdp"
        android:layout_marginBottom="@dimen/_15sdp"
        android:layout_gravity="bottom"
        android:gravity="center"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/ivWebsite"
            android:layout_width="8dp"
            android:layout_height="@dimen/_6sdp"
            android:src="@drawable/s3"
            android:layout_gravity="center"
            android:visibility="gone"
            app:tint="@color/white" />

        <TextView
            android:id="@+id/tvframeweb"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_4sdp"
            android:ellipsize="end"
            android:gravity="center"
            android:maxLines="1"
            android:textColor="@color/ColorBlack"
            android:textSize="@dimen/_6sdp"
            android:visibility="gone" />

    </LinearLayout>
    <LinearLayout
        android:id="@+id/ll_call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginStart="@dimen/_50sdp"
        android:layout_marginBottom="@dimen/_23sdp"
        android:layout_gravity="bottom|center"
        android:gravity="center"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/ivCall"
            android:layout_width="@dimen/_10sdp"
            android:layout_height="8dp"
            android:src="@drawable/s1"
            android:layout_gravity="center"
            android:visibility="gone"
            app:tint="@color/white" />

        <TextView
            android:id="@+id/tvframephone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_9sdp"
            android:ellipsize="end"
            android:gravity="center"
            android:maxLines="1"
            android:textColor="@color/white"
            android:textSize="8dp"
            android:visibility="gone" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_marginBottom="2dp"
        android:layout_gravity="bottom|center"
        android:gravity="center"
        android:orientation="horizontal">


        <ImageView
            android:id="@+id/ivLocation"
            android:layout_width="@dimen/_4sdp"
            android:layout_height="@dimen/_6sdp"
            android:src="@drawable/s4"
            android:visibility="gone"
            app:tint="@color/white" />

        <TextView
            android:id="@+id/tvframelocation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_4sdp"
            android:ellipsize="end"
            android:gravity="center"
            android:maxLines="1"
            android:textColor="@color/white"
            android:textSize="@dimen/_6sdp"
            android:visibility="gone" />

    </LinearLayout>


